{
    "name": "maksimdimitrov/mdim-amqp-laravel",
    "description": "Laravel service provider for php-amqplib to use RabbitMQ",
    "keywords": ["rabbitmq", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Dimitrov"
        }
    ],
    "autoload": {
        "psr-4": {
            "MdimAMQPLaravel\\": "MdimAMQPLaravel/"
        }
    },
    "require": {
        "php-amqplib/php-amqplib": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MdimAMQPLaravel\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}